<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
 xmlns="urn:nhibernate-mapping-2.2"
 assembly="Events.NHibernateDataProvider"
 namespace="Events.Business.Models"
 auto-import="true">
  <class name="University, Abitcareer.Business.Models" table="Events">
    <id name="Id" access="property" type="int">
      <generator class="assigned"></generator>
    </id>

    <property name="Name" access="property" type="string"></property>

    <property name="NameEN" access="property" type="string"></property>

    <property name="CityId" access="property" type="int"></property>

    <property name="Rating" access="property" type="int"></property>

    <property name="Link" access="property" type="string"></property>
  </class>
</hibernate-mapping>